@model App.ControllerModels.SecurityModel
@{
    ViewData["Title"] = "Application";
}
<link rel="stylesheet" href="css/extra.css" />

<div class="container">
    <div class="row">
        <button id="InfoButton" class="btn btn-info btn-lg" style="visibility:hidden">
			<i class="fa fa-info-circle"></i>
		</button>
		<button id="EdgeButton" class="btn btn-primary btn-lg" style="visibility:hidden">
            <i class="fa fa-power-off"></i>
        </button>
        <h4 class="applicationDisplay" style="visibility:hidden;display:none">
            <span id="counter" style="font-size: 18px; margin-right: 5px; vertical-align: middle;">0</span>
            <i class="fa fa-link" style="font-size: 18px; vertical-align: middle;"></i>
            <br />
            <br />
            <br />
            <input type="text" id="valueInput" name="valueInput" />
            <button id="addValuePlusButton" type="button" onclick="myFunction()">+</button>
            <br /><br />
            <!--<span id="speaker-logos" style="font-size: 18px; margin-right: 5px; vertical-align: middle;">👄</span>
            <br /><br />-->
            <span id="value-logos" style="font-size: 18px; margin-right: 5px; vertical-align: middle;">💲</span>
            <span id="value-value" style="font-size: 18px; margin-right: 5px; vertical-align: middle;"></span>
            <br /><br />
            <!--<span id="d2c"></span>-->
        </h4>
    </div>
</div>

<script src="js/extra.js" type="text/javascript"></script>